Film "image" 
	"integer xresolution" [900] "integer yresolution" [900]
    "string filename" "ubersphere.png"
Integrator "path" "integer maxdepth" [1]
Sampler "halton" "integer pixelsamples" [32] 

LookAt 0 3 8  0 .8 0   0 1 0
Camera "perspective" "float fov" [22]

WorldBegin


AttributeBegin
  LightSource "infinite" "color L" [.5 .5 .5]
AttributeEnd

AttributeBegin
Material "uber" "rgb Kd" [0 0 0] "rgb Ks" [1 1 1] "float roughness" [0.1] "bool remaproughness" "false"
Translate 0 1 0
Rotate 60 1 1 1
Scale .9 .9 .9
Shape "sphere"
AttributeEnd


WorldEnd

